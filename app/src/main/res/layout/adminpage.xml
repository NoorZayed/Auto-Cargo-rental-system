<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/add"
    tools:context=".adminpage">

    <LinearLayout
        android:id="@+id/linearLayout9"
        android:layout_width="409dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:background="#374A55"
        android:backgroundTint="#3C021D"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="33dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/logo" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_weight="1"
            android:text="Auto Cargo"
            android:textAllCaps="false"
            android:textColor="@android:color/holo_orange_dark"
            android:textSize="30sp"
            android:textStyle="italic" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Admin Page"
            android:textAlignment="textEnd"
            android:textColor="#F3EEEE"
            android:textStyle="bold" />


    </LinearLayout>

    <Button
        android:id="@+id/custom_btn"
        android:layout_width="161dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="120dp"
        android:width="180dp"
        android:backgroundTint="@color/red"

        android:elegantTextHeight="true"
        android:text="Customers"
        android:textSize="14sp"
        android:textStyle="bold"
        app:icon="@android:drawable/ic_menu_info_details"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/logout_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rental_btn"
        app:layout_constraintVertical_bias="0.62" />

    <Button
        android:id="@+id/logout_btn"
        android:layout_width="158dp"
        android:layout_height="117dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="120dp"
        android:width="180dp"
        android:backgroundTint="@color/red"
        android:elegantTextHeight="true"
        android:text="Log Out"
        android:textSize="14sp"
        android:textStyle="bold"
        app:icon="@drawable/logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/custom_btn"
        app:layout_constraintTop_toBottomOf="@+id/add_btn"
        tools:ignore="MissingConstraints"
        tools:text="Log Out" />

    <Button
        android:id="@+id/allcars_btn"
        android:layout_width="157dp"
        android:layout_height="123dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="176dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="30dp"
        android:width="180dp"
        android:backgroundTint="@color/red"
        android:elegantTextHeight="true"
        android:text="All Cars"
        android:textSize="14sp"
        android:textStyle="bold"
        app:icon="@android:drawable/ic_menu_search"
        app:layout_constraintBottom_toTopOf="@+id/rental_btn"
        app:layout_constraintEnd_toStartOf="@+id/delete_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/rental_btn"
        android:layout_width="155dp"
        android:layout_height="116dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="43dp"
        android:width="180dp"
        android:backgroundTint="@color/red"
        android:elegantTextHeight="true"
        android:text="Rental Cars"

        android:textSize="14sp"
        android:textStyle="bold"
        app:icon="@drawable/cars"
        app:layout_constraintBottom_toTopOf="@+id/custom_btn"
        app:layout_constraintEnd_toStartOf="@+id/add_btn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/allcars_btn"
        tools:text="Rental Cars" />

    <Button
        android:id="@+id/delete_btn"
        android:layout_width="164dp"
        android:layout_height="118dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="176dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="30dp"
        android:width="180dp"
        android:backgroundTint="@color/red"
        android:elegantTextHeight="true"
        android:text="Edit"
        android:textSize="14sp"
        android:textStyle="bold"
        app:icon="@android:drawable/ic_menu_manage"
        app:layout_constraintBottom_toTopOf="@+id/add_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/allcars_btn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/add_btn"
        android:layout_width="164dp"
        android:layout_height="112dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="44dp"
        android:width="180dp"
        android:backgroundTint="@color/red"
        android:elegantTextHeight="true"
        android:text="Add"
        android:textSize="14sp"
        android:textStyle="bold"
        app:icon="@android:drawable/ic_menu_add"
        app:layout_constraintBottom_toTopOf="@+id/logout_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rental_btn"
        app:layout_constraintTop_toBottomOf="@+id/delete_btn" />


</androidx.constraintlayout.widget.ConstraintLayout>
